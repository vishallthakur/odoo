<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_website_advance_search_form" model="ir.ui.view">
        <field name="name">website.advance_search.form</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Advance Website Search" name="page_advance_website_search">
                    <field name="advance_search_ids" context="{'default_website_id': id}">
                        <tree editable="bottom">
                            <field name="sequence" widget="handle"/>
                            <field name="model_id" />
                            <field name="field_ids" widget="many2many_tags" domain="[('model_id', '=', model_id)]" />
                            <field name="is_active" />
                        </tree>
                    </field>
                </page>
            </notebook>
        </field>
    </record>

</odoo>